package com.mdhalim.soft.GettersAndSetters;

public class Dog {

	 int speed =90;
	
	public int getSpeed() {
		return speed;
	}
	
	void setSpeed(int value) {
		speed =value;
	}
	
	
	
	int power =100;
	int getPower() {
		return power;
	}

	void setPower(int value) {
		power = value;
	}

	
	
	void run() {
		System.out.println("RUN....");
	}
	
	void sleep() {
		System.out.println("SLEEP....");
	}
}
