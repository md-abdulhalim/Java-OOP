package com.mdhalim.soft.ConstructorsInJava;

public class Tiger {

	//constructor name must be same as its class name.
	private int speed =40;
	public Tiger() {
		
	}
	
	public int getSpeed() {
		return speed;
	}
	
	public void setSpeed(int speed) {
		this.speed =speed;
	}
	
	public void fight() {
		System.out.println("FIGHT....");
	}
}
