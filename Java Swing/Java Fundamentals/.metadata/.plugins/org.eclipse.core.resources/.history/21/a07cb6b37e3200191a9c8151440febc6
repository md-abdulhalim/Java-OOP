package com.mdhalim.soft.LogicalOperators;

public class LogicalOperator {

	public static void main(String[] args) {
		
		// Logical operator 
		
		// AND 
		//OR
		//NOT
		//XOR 
		
		int salary = 2000;
		System.out.println(salary);
		
		boolean isHired =(salary >= 1000);
		System.out.println(isHired);
		

	}

}
